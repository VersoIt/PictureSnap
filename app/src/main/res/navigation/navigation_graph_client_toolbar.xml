<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph_client_toolbar"
    app:startDestination="@id/unregisteredToolbar">
    <fragment
        android:id="@+id/unregisteredToolbar"
        android:name="ru.verso.picturesnap.presentation.fragments.client.UnregisteredToolbar"
        android:label="fragment_unregistered_toolbar"
        tools:layout="@layout/fragment_unregistered_toolbar" >
        <action
            android:id="@+id/action_unregisteredToolbar_to_clientToolbar"
            app:destination="@id/clientToolbar" />
        <action
            android:id="@+id/action_unregisteredToolbar_to_photographToolbar"
            app:destination="@id/photographToolbar" />
    </fragment>
    <fragment
        android:id="@+id/clientToolbar"
        android:name="ru.verso.picturesnap.presentation.fragments.client.ClientToolbar"
        android:label="ClientToolbar" >
        <action
            android:id="@+id/action_clientToolbar_to_clientToolbarBackable"
            app:destination="@id/clientToolbarBackable" />
    </fragment>
    <fragment
        android:id="@+id/clientToolbarBackable"
        android:name="ru.verso.picturesnap.presentation.fragments.client.ClientToolbarBackable"
        android:label="ClientToolbarBackable" >
        <action
            android:id="@+id/action_clientToolbarBackable_to_clientToolbar"
            app:destination="@id/clientToolbar" />
    </fragment>
    <fragment
        android:id="@+id/photographToolbar"
        android:name="ru.verso.picturesnap.presentation.fragments.photograph.PhotographToolbar"
        android:label="PhotographToolbar" >
        <action
            android:id="@+id/action_photographToolbar_to_photographToolbarBackable"
            app:destination="@id/photographToolbarBackable" />
    </fragment>
    <fragment
        android:id="@+id/photographToolbarBackable"
        android:name="ru.verso.picturesnap.presentation.fragments.photograph.PhotographToolbarBackable"
        android:label="fragment_photograph_toolbar_backable"
        tools:layout="@layout/fragment_photograph_toolbar_backable" >
        <action
            android:id="@+id/action_photographToolbarBackable_to_photographToolbar"
            app:destination="@id/photographToolbar" />
    </fragment>
</navigation>